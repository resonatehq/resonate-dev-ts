{
  "name": "@resonatehq/dev",
  "version": "0.1.3",
  "description": "A TypeScript development server implementation for the Resonate distributed async/await framework.",
  "repository": {
    "url": "https://github.com/resonatehq/resonate-dev-ts"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "check": "biome check",
    "format": "biome check --write",
    "prepare": "tsc",
    "prepack": "tsc",
    "test": "bun test",
    "type-check": "npx tsc --noEmit",
    "release": "bun run scripts/release.ts"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@types/bun": "latest",
    "open": "^11.0.0"
  },
  "dependencies": {
    "cron-parser": "^5.5.0"
  }
}
